<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>js</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css" integrity="sha512-Evv84Mr4kqVGRNSgIGL/F/aIDqQb7xQ2vcrdIwxfjThSH8CSR7PBEakCr51Ck+w+/U6swU2Im1vVX0SVk9ABhg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4Q6Gf2aSP4eDXB8Miphtr37CMZZQ5oXLH2yaXMJ2w8e2ZtHTl7GptT4jmndRuHDT" crossorigin="anonymous">
    
    <style>
        #result{
            padding-left: 40%;
            color: black;
            margin-top: 5%;
            
            
            
        }
        .container{
            border-radius: 2rem;
        }
        button{
            margin-left: 40%;
        }
    </style>
    

</head>
<body>
    
   <div class="container border bg-info my-5">
     <h1>12th Class Result</h1>
     <label for="name" class="form-label">Name :</label>
     <input type="text" id="name" class="form-control mb-5 p-3"placeholder="enter you name">
     <label for="roll" class="form-label">Roll Number :</label>
     <input type="number" id="roll" class="form-control mb-5 p-3" placeholder="enter you name">
     <button type="button" class="btn btn-dark btn-lg px-5">Result</button>
     <div id="result"></div>
   </div>
   
    
   

    <script src="https://cdn.jsdelivr.net/npm/axios@1.6.7/dist/axios.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/js/bootstrap.bundle.min.js" integrity="sha384-j1CDi7MgGQ12Z7Qab0qlWQ/Qqz24Gc6BM0thvEMVjHnfYGF0rmFCozFSxQBxwHKO" crossorigin="anonymous"></script>
    <script  >
        function Instructor(name,rollnumber,...marks){
        this.name=name;
        this.rollnumber=rollnumber;
        this.marks=marks;
        }

        Instructor.prototype.percentage=function(){
        let per=0
        for(let i=0;i<this.marks.length;i++){
            per+=this.marks[i]
            
        }
        return `result : ${per*100/500}%`
        }



        let o1= new Instructor("Vikash Saini",2600315,90,95,85,80,100);
        let o2= new Instructor("Pankaj Saini",2600316,80,50,80,80,80);
        let o3= new Instructor("jyoti saini",2600317,100,70,60,50,100);
        let o4= new Instructor("Sanjay Saini",2600318,100,100,85,80,100);
        let o5= new Instructor("vikash saini",2600315,90,95,85,80,100);
        let o6= new Instructor("vikash saini",2600315,90,95,85,80,100);
        let o7= new Instructor("vikash saini",2600315,90,95,85,80,100);
        let o8= new Instructor("vikash saini",2600315,90,95,85,80,100);
        let o9= new Instructor("vikash saini",2600315,90,95,85,80,100);
        let o10= new Instructor("vikash saini",2600315,90,95,85,80,100);
        let data=[o1,o2,o3,o4,o5,o6,o7,o8,o9,o10]
        let student=document.querySelector("#name")
        let roll=document.querySelector("#roll")
        let button=document.querySelector("button");
        let result=document.querySelector("#result");
        function work(){
        for (i of data){
            if(student.value==i.name && roll.value==i.rollnumber){
            
            
               return  result.innerText=`
            hindi : ${i.marks[0]}
            english : ${i.marks[1]}
            math : ${i.marks[2]}
            chemistry : ${i.marks[3]}
            physics : ${i.marks[4]}

            ${i.percentage()}
            
            `
                
            
            }
            if(student.value!=i.name || roll.value!=i.rollnumber){
                result.innerText=`incorrect name or roll number`
            }
        }
        }
        button.addEventListener("click",work)

        
    </script>
</body>
</html>